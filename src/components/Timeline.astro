---
import { education, experience } from "consts";
import Article from "./ui/Article.astro";
import Journy from "./ui/journy.astro";
---

<Article title="Timeline" sectionClass="">

        <Journy icon={experience[0].icon}  time={experience[0].duration} title={experience[0].role}  uni={experience[0].description} clg={experience[0].company}  type={experience[0].type}/>
        {education.map(edu =>  <Journy icon={edu.icon} last={edu.year === "2015"}  time={edu.year} title={edu.degree}  uni={edu.university} clg={edu.institution} type={edu.type} />)}
     
</Article>